{
  "name": "reactnativehttptesting",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "concurrently \"react-native run-android\" \"npm:server\"",
    "ios": "concurrently \"react-native run-ios\" \"npm:server\"",
    "start": "react-native start",
    "server": "json-server --watch db.json",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "axios": "^0.24.0",
    "concurrently": "^6.5.1",
    "json-server": "^0.17.0",
    "react": "17.0.2",
    "react-native": "0.66.4",
    "react-native-dotenv": "^3.3.1",
    "react-native-linear-gradient": "^2.5.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.16.5",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/react-native": "^9.0.0",
    "babel-jest": "^27.4.5",
    "eslint": "7.14.0",
    "jest": "^27.4.5",
    "metro-react-native-babel-preset": "^0.66.2",
    "msw": "^0.36.3",
    "promise-polyfill": "^8.2.1",
    "react-test-renderer": "17.0.2",
    "ts-jest": "^27.1.2",
    "whatwg-fetch": "^3.6.2"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native|react-native-vector-icons))"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  }
}
